<section>
    <div class="container mt-5">
<table class="table">
  <thead class="thead-light">
    <tr>
      <th scope="col">No</th>
      <th scope="col">Title</th>
      <th scope="col">Catagory</th>
      <th scope="col">Description</th>
      <th scope="col">Image</th>
    </tr>
  </thead>
  <tbody>
    {{#each product}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.catagory}}</td>
      <td>{{this.description}}</td>
      <td></td>
    </tr> 
    {{/each}} 
  </tbody>
</table>
    </div>
</section>